{
  "build": {
    "release": {
      "android": {
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "credentialsSource": "local",
        "buildConfiguration": "Release"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "credentialsSource": "local"
    },
    "developmentRemoteCredentials": {
      "developmentClient": true,
      "distribution": "internal"
    }
  },
  "cli": {
    "version": ">= 0.40.0"
  }
}
